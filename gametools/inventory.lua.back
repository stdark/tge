rt=25,range=9,clip=0,
level=1,material=10,price=30,weight=2,
quest=0,special=0,artifact=0,relic=0,
story="Скверно обструганная кривоватая палка."},
{id=102,
x=1,y=35,w=1,h=8,
skill="staff",
class="staff",
subclass="battlestaff",
classtitle="посох",
subclasstitle="боевой посох",
title="Боевой шест",
